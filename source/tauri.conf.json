{
  "package": {
    "productName": "tauri-boilerplate",
    "version": "1.0.0"
  },
  "tauri": {
    "windows": [
      {
        "title": "tauri-boilerplate",
        "height": 600,
        "width": 800,
        "transparent": true,
        "fullscreen": false,
        "alwaysOnTop": true,
        "resizable": true,
        "visible": true
      }
    ],
    "bundle": {
      "active": true,
      "identifier": "com.meslzy.dev",
      "category": "DeveloperTool",
      "longDescription": "long description",
      "shortDescription": "short description",
      "copyright": "copyright",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "targets": "all",
      "windows": {
        "timestampUrl": "",
        "digestAlgorithm": "sha256",
        "certificateThumbprint": null
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "entitlements": null,
        "signingIdentity": null,
        "providerShortName": null
      },
      "deb": {
        "depends": []
      }
    },
    "allowlist": {
      "all": true
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    }
  },
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../packages/webview/dist"
  }
}